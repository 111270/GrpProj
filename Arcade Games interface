    private void jButton1MouseClicked(java.awt.event.MouseEvent evt) {                                      
        jButton1.setVisible(true);
        this.dispose();
        
        //going back to the dashboard
        GenreDashboard dash = new GenreDashboard();
        dash.setVisible(true);
    }                                     

