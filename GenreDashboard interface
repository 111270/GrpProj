    private void jLabel1MouseClicked(java.awt.event.MouseEvent evt) {                                     
        jLabel1.setVisible(true);
        this.dispose();
        
        //linking new interface.
        ArcadeGames arc = new ArcadeGames();
        arc.setVisible(true);
        
        
    }                                    

    private void jLabel2MouseClicked(java.awt.event.MouseEvent evt) {                                     
       jLabel2.setVisible(true);
       this.dispose();
       
       //linking Combat games interface
       CombatGames com = new CombatGames ();
       com.setVisible(true);
    }                                    

    private void jLabel3MouseClicked(java.awt.event.MouseEvent evt) {                                     
        jLabel3.setVisible(true);
        this.dispose();
        
        //linking to RacingGames interface
        RacingGames race = new RacingGames();
        race.setVisible(true);
    }                                    

    private void jLabel4MouseClicked(java.awt.event.MouseEvent evt) {                                     
        jLabel4.setVisible(true);
        this.dispose();
        
        //linking to StoryGames interface
        StoryGames stry = new StoryGames();
        stry.setVisible(true);
    }                                    

