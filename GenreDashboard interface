    private void jRadioButtonMenuItem1MouseClicked(java.awt.event.MouseEvent evt) {                                                   
        // linking to another interface 
        jRadioButtonMenuItem1.setVisible(true);
        this.dispose();
        
        ArcadeGames arcade = new ArcadeGames();
        arcade.setVisible(true);
    }                                                  

